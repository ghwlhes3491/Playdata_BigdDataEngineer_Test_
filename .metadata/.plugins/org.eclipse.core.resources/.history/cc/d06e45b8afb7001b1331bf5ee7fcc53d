package com.work.view;

public class GuGuDanTest {
	public static void main(String[] args) {
		//1. for구문을 활용한 구구단
		for(int i = 2; i <= 9 ; i++) {
			System.out.print(i+"단\t");
			
		for(i = 1; i <= 9 ; i++) {
			for(int j=2;j<=9 ; j++) {
				int result=j+i;
				System.out.print(j+" * "+i +" = "+result+"\t");
			}System.out.println();
		}
			
		}
		
		//4. 반복문을 활용하여 짝수 구구단만 출력	
	}
	
	

}
