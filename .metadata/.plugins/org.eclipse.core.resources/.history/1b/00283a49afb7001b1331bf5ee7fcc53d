package com.work.view;

public class GuGuDanTest {
	public static void main(String[] args) {
		//for구문 활용
		for(int i =2; i<=9 ; i++) {
			System.out.print(i+"단\t");
		for(int i=1;i<=9 ; i++) {
			for(int j=2;j<=9 ; j++) {
				int result=j+i;
				System.out.print(j+" * "+i +"= "+result+"\n");
				
			}
		}
			
		}
	}
	
	

}
